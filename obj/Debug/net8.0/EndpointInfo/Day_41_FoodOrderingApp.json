            }
          },
          "orderAmount": {
            "type": "number",
            "format": "double"
          },
          "deliveryPartnerId": {
            "type": "integer",
            "format": "int32"
          },
          "deliveryPartner": {
            "$ref": "#/components/schemas/DeliveryPartner"
          },
          "paymentMethodId": {
            "type": "integer",
            "format": "int32"
          },
          "paymentMethod": {
            "$ref": "#/components/schemas/PaymentMethod"
          },
          "discount": {
            "type": "number",
            "format": "double"
          },
          "finalAmount": {
            "type": "number",
            "format": "double"
          }
        },
        "additionalProperties": false
      },
      "OrderItem": {
        "required": [
          "menuItemId",
          "quantity"
        ],
        "type": "object",
        "properties": {
          "orderItemId": {
            "type": "integer",
            "format": "int32"
          },
          "menuItemId": {
            "type": "integer",
            "format": "int32"
          },
          "menuItem": {
            "$ref": "#/components/schemas/MenuItem"
          },
          "quantity": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "PaymentMethod": {
        "required": [
          "payments"
        ],
        "type": "object",
        "properties": {
          "paymentMethodId": {
            "type": "integer",
            "format": "int32"
          },
          "payments": {
            "$ref": "#/components/schemas/Payments"
          }
        },
        "additionalProperties": false
      },
      "Payments": {
        "enum": [
          0,
          1,
          2,
          3,
          4
        ],
        "type": "integer",
        "format": "int32"
      },
      "WeatherForecast": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date"
          },
          "temperatureC": {
            "type": "integer",
            "format": "int32"
          },
          "temperatureF": {
            "type": "integer",
            "format": "int32",
