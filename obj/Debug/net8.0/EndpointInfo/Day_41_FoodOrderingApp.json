  "components": {
    "schemas": {
      "DeliveryPartner": {
        "required": [
          "deliveryPartnerID",
          "deliveryPartnerName",
          "deliveryPartnerPhone",
          "deliveryPartnerRating"
        ],
        "type": "object",
        "properties": {
          "deliveryPartnerID": {
            "type": "integer",
            "format": "int32"
          },
          "deliveryPartnerName": {
            "minLength": 1,
            "type": "string"
          },
          "deliveryPartnerPhone": {
            "minLength": 1,
            "pattern": "^[0-9]{3}-[0-9]{3}$",
            "type": "string"
          },
          "deliveryPartnerRating": {
            "type": "integer",
            "format": "int32"
          }
        },
        "additionalProperties": false
      },
      "MenuItem": {
